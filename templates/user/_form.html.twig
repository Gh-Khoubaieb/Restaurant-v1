


{{ form_start(form) }}
<div class="row">
    <div class="col-md-4">{{ form_row(form.firstname) }}</div>
    <div class="col-md-4">{{ form_row(form.lastname) }}</div>
    <div class="col-md-4">{{ form_row(form.birthdate) }}</div>
</div>

<div class="row">

    <div class="col-md-2">{{ form_row(form.address) }}</div>
    <div class="col-md-2">{{ form_row(form.city) }}</div>
    <div class="col-md-1">{{ form_row(form.zipcode) }}</div>
    <div class="col-md-2">{{ form_row(form.country)}}</div>
    <div class="col-md-2">{{ form_row(form.phone) }}</div>
</div>

<div class="row">
    <div class="col-md-2">{{ form_row(form.email) }}</div>
    <div class="col-md-2">{{ form_row(form.password) }}</div>
</div>
{{ form_rest(form) }}
<button class="btn btn-primary" >{{ button |default('Cr√©er le compte') }}</button>
<button class="btn btn-danger" type="reset" >Annuler</button>
{{ form_end(form) }}